{
  "extends": ["jam/typescript/base"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "warn",
    "global-require": "off",
    "import/no-dynamic-require": "off",
    "no-empty-function": "warn",
    "lines-between-class-members": [
      "error",
      "always",
      { "exceptAfterSingleLine": true }
    ]
  }
}

{
  "name": "rtd",
  "version": "0.1.12",
  "private": true,
  "description": "example using rtd sphinxdoc platform",
  "keywords": [
    "rtd",
    "read-the-docs",
    "sphinx",
    "example",
    "jsdoc"
  ],
  "homepage": "https://github.com/codejamninja/rtd",
  "bugs": {
    "url": "https://github.com/codejamninja/rtd/issues",
    "email": "jam@codejam.ninja"
  },
  "license": "MIT",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/codejamninja/rtd"
  },
  "scripts": {
    "build": "babel src -d lib",
    "clean": "git clean -fXd -e \\!node_modules -e \\!node_modules/**/*",
    "docs:build": "sphinxdoc build",
    "docs:start": "sphinxdoc start",
    "format": "prettier --write ./src/**/*.js",
    "lint": "npm run format && eslint ./",
    "lint:fix": "npm run format && eslint --fix ./",
    "prepublish": "npm run clean && npm run build",
    "start": "babel-node src"
  },
  "dependencies": {
    "@sphinxdoc/cli": "^0.1.12",
    "@sphinxdoc/rtd": "^0.1.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-preset-everything": "^0.2.6",
    "eslint": "^5.5.0",
    "eslint-config-jam": "^0.1.2"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "eslintIgnore": [
    "lib"
  ],
  "sphinxdoc": {
    "platformName": "rtd",
    "open": true
  },
  "gitHead": "5c78d3cf7c6b8f97dd99eb49010a1e2f6721d585"
}

{
  "name": "@sphinxdoc/cli",
  "version": "0.1.30",
  "description": "sphinxdoc cli",
  "keywords": [
    "sphinxdoc",
    "sphinx",
    "python",
    "doc",
    "docs",
    "documentation",
    "jsdoc",
    "generate",
    "generator"
  ],
  "homepage": "https://github.com/codejamninja/sphinxdoc",
  "bugs": {
    "url": "https://github.com/codejamninja/sphinxdoc/issues",
    "email": "jam@codejam.ninja"
  },
  "license": "MIT",
  "author": {
    "name": "Jam Risser",
    "email": "jam@codejam.ninja",
    "url": "https://codejam.ninja"
  },
  "files": [
    "lib",
    "bin"
  ],
  "main": "lib/index.js",
  "bin": {
    "sphinxdoc": "bin/bin.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/codejamninja/sphinxdoc"
  },
  "scripts": {
    "build": "npm run test && babel src -d lib",
    "clean": "git clean -fXd -e \\!node_modules -e \\!node_modules/**/*",
    "format": "prettier --write ./src/**/*.js",
    "lint": "npm run format && eslint ./",
    "lint:fix": "npm run format && eslint --fix ./",
    "prepublish": "npm run clean && npm run build",
    "start": "babel-node src/bin",
    "test": "npm run lint"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@sphinxdoc/core": "^0.1.30",
    "commander": "^2.19.0",
    "err": "^2.1.10",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-preset-everything": "^0.2.6",
    "eslint": "^5.5.0",
    "eslint-config-jam": "^0.1.2"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "eslintIgnore": [
    "lib"
  ],
  "gitHead": "43ca46b5fc7f579fe6977dbbdf19574b9beb220d"
}
